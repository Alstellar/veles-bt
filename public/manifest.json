{
  "manifest_version": 3,
  "name": "Veles Helper",
  "version": "1.0.0",
  "description": "Конфигуратор параметров для поиска эффективных стратегий на Veles.Finance",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "cookies",
    "tabs"
  ],
  "icons": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
  "host_permissions": [
    "https://veles.finance/*"
  ]
}